{
  "name": "websub",
  "version": "0.6.0",
  "description": "Basic WebSub subscriber module.",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepack": "yarn run build",
    "prepublish": "yarn run test",
    "test": "yarn run check && yarn run lint",
    "check": "yarn run compile --noEmit",
    "lint": "xo ./src",
    "clean": "rimraf lib",
    "compile": "tsc",
    "build": "yarn run clean && yarn run compile",
    "dev": "yarn run clean && yarn run compile --watch"
  },
  "repository": "git@github.com:lolPants/env.git",
  "author": "Jack Baron <jackmbaron@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "xo": "^0.50.0"
  }
}
